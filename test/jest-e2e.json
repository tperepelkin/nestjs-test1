{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "my/lib1/(.*)": "<rootDir>/../libs/lib1/src/$1",
    "my/lib1": "<rootDir>/../libs/lib1/src"
  }
}